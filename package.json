		"typescript": "^5.2.2",
		"vite": "^4.5.3",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.4",
		"wrangler": "^3.60.3"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/poppins": "^5.0.13",
		"@neoconfetti/svelte": "^1.0.0"
